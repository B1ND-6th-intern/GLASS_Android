<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <PreferenceCategory
        app:key="privateInfo"
        app:title="개인 설정" >
        <ListPreference
            android:entries="@array/my_job"
            android:entryValues="@array/permission"
            app:key="job"
            app:title="직업 선택" />
        <EditTextPreference
            app:key="email"
            app:title="이메일 변경" />
        <EditTextPreference
            app:key="password"
            app:title="비밀번호 변경" />
    </PreferenceCategory>

    <PreferenceCategory
        app:key="etc"
        app:title="기타 설정">
        <SwitchPreferenceCompat
            app:key="test"
            app:title="테스트. 뭐해야 하나" />
    </PreferenceCategory>

</PreferenceScreen>